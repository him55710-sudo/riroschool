# .env.example

# Database connection
DATABASE_URL="file:./dev.db"

# AI Model
GEMINI_API_KEY="your_gemini_api_key_here"
GEMINI_CHAT_MODEL="gemini-2.5-flash"
GEMINI_CHAT_FALLBACK_MODELS="gemini-2.5-flash"
GEMINI_WRITE_MODEL="gemini-2.5-flash"
GEMINI_WRITE_FALLBACK_MODELS="gemini-2.5-flash"
GEMINI_CHAT_MAX_OUTPUT_TOKENS="8192"
GEMINI_CHAT_MAX_SEGMENTS="24"
OLLAMA_BASE_URL="http://127.0.0.1:11434"
OLLAMA_MODEL="qwen2.5:7b"
OLLAMA_FALLBACK_MODEL="qwen2.5:14b"
OLLAMA_TIMEOUT_MS="240000"
OLLAMA_NUM_PREDICT="5200"
OLLAMA_ENABLED="0"
OLLAMA_DISABLED="1"
ENABLE_LOCAL_JOB_FALLBACK="0"

# Storage
STORAGE_BUCKET="portfolio_pdfs"
AWS_ACCESS_KEY_ID=""
AWS_SECRET_ACCESS_KEY=""

# Authentication (NextAuth + Google)
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your_nextauth_secret_here"
GOOGLE_CLIENT_ID="your_google_client_id_here"
GOOGLE_CLIENT_SECRET="your_google_client_secret_here"
ADMIN_EMAILS="admin@example.com"
